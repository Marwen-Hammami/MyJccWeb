{% if true%}
	<!DOCTYPE html>
	<html>
		<head>
			<meta charset="UTF-8">
			<title>
				{% block title %}Welcome!
				{% endblock %}
			</title>
			<meta
			name="viewport" content="width=device-width, initial-scale=1.0"/>
			<!-- App favicon -->
			<link rel="shortcut icon" href="{{ asset('images/myjccLogo.png') }}"/>

			{{ encore_entry_link_tags('app') }}
			<link href="{{ asset('css/bootstrap.min.css') }}" id="bootstrap-style" rel="stylesheet" type="text/css"/>
			<link href="{{ asset('css/icons.min.css') }}" rel="stylesheet" type="text/css"/>
			<link href="{{ asset('css/app.min.css') }}" id="app-style" rel="stylesheet" type="text/css"/>
			{% block stylesheets %}
				{# ici par exemple les tableaux #}
			{% endblock %}
		</head>
		<body data-sidebar="dark">
			<div id="layout-wrapper">
				{% block loading %}
					{# {{ include('templateBackOffice/loading.html.twig') }} #}
				{% endblock %}
				{{ include('templateBackOffice/topBar.html.twig') }}
				{{ include('templateBackOffice/leftSideBar.html.twig') }}

				<div class="main-content">
					<div class="page-content">
						<div class="container-fluid">

							{% block body %}
								<p>
									contenu
								</p>
							{% endblock %}

							{{ include('templateBackOffice/footer.html.twig') }}
						</div>
					</div>
				</div>

			</div>
			{# JS Scripts #}
			{{ encore_entry_script_tags('app') }}
			<script src="{{ asset('libs/jquery/jquery.min.js') }}"></script>
			<script src="{{ asset('libs/bootstrap/js/bootstrap.bundle.min.js') }}"></script>
			<script src="{{ asset('libs/metismenu/metisMenu.min.js') }}"></script>
			<script src="{{ asset('libs/simplebar/simplebar.min.js') }}"></script>
			<script src="{{ asset('libs/node-waves/waves.min.js') }}"></script>
			<script src="{{ asset('js/app.js') }}"></script>
			{% block javascripts %}{% endblock %}
		</body>
	</html>
{% else %}
	Non connect√©.
	<a href="{{ path('app_t_m_p_session_loggOut') }}" id="session-check"></a>
	<script>
		window.addEventListener('load', function () {
var logoutLink = document.getElementById('session-check');
logoutLink.click();
});
	</script>
{% endif %}